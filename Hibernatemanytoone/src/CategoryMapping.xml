<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.nttdata.Category">
        <id name="categoryId" >

        </id>
        <property name="categoryName"></property>
        <many-to-one name="product" column="pro" class="com.nttdata.Product" cascade="all">
        </many-to-one>
         </class>
         </hibernate-mapping>
        